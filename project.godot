[application]

config/name="Desmanguezados"
run/main_scene="uid://4k8drpikovn2"
config/features=PackedStringArray("4.4", "GL Compatibility")
config/icon="uid://csn05g2ocwtcj"

[autoload]

Globais="*res://Scripts/Autoloads/globais.gd"
SceneManager="*res://Scripts/Autoloads/sceneManager.gd"
InputManager="*res://Scripts/Autoloads/inputManager.gd"
SaveManager="*res://Scripts/Autoloads/saveManager.gd"
LevelManager="*res://Scripts/Autoloads/levelManager.gd"

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/window_width_override=1000
window/size/window_height_override=600
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/TileMapDual/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Cenas/": "orange",
"res://PreSets/": "pink",
"res://PreSets/Siri/blue/": "blue",
"res://PreSets/Siri/red/": "red",
"res://Scripts/": "purple",
"res://addons/": "blue"
}

[global_group]

